<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="500.0" minWidth="600.0" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bbb.m306.tictactoe.HostController">
   <children>
      <Label text="Waiting for Player to Join">
         <font>
            <Font size="30.0" />
         </font></Label>
      <Label fx:id="ip_lbl" text="IP">
         <font>
            <Font size="30.0" />
         </font></Label>
      <Button fx:id="cancel_btn" mnemonicParsing="false" styleClass="btn" text="Cancel">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin></Button>
   </children>
</VBox>
